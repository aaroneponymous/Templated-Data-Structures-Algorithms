{
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "black",
        "background": "white",
        "opacity": 50,
        "iconColour": "blue",
        "gutterIcon": true,
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "foreground": "black",
            "background": "white",
            "iconColour": "white",
        },
        "NOTE": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "white",
        },
        "COMMENT": {
            "icon": "note",
            "foreground": "black",
            "background": "yellow",
            "iconColour": "yellow",
        },
        "FIX": {
            "foreground": "black",
            "background": "yellow",
            "iconColour": "yellow",
        },
        "BUG": {
            "foreground": "black",
            "background": "red",
            "iconColour": "red",
        },
        "[ ]": {
            "icon": "check",
            "foreground": "black",
            "background": "white",
            "iconColour": "yellow",
        },
        "[x]": {
            "icon": "check",
            "foreground": "white",
            "background": "green",
            "iconColour": "green",
        }
    },

    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIX",
        "TODO",
        "NOTE",
        "COMMENT",
        "[ ]",
        "[x]"
    ],

    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*\\*?|^)\\s*($TAGS)|(/\\*\\*?\\s*.*\\n(\\s*\\*\\s?.*\\n)+\\s*\\*/)",
